<div class="users-page">
  <div class="page-title">
    <div class="clearfix">
      <div class="pull-left">
        <h2>
          Usuários
          <small>
            <%= @users.total_count %> usuários encontrados
          </small>
        </h2>
      </div>
      <div class="pull-right">
        <a href="<%= new_admin_user_path %>" class="btn btn-success">
          <i class="fa fa-plus-square"></i>
          Novo
        </a>
      </div>
    </div>
  </div>
  <div>
  <%= render "admin/shared/notice" %>
  <%= render "search" %>
  <%= render "list" %>
</div>
